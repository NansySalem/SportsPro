@model SportsPro.Models.Technician
@{
    ViewData["Title"] = "Technician List";
}
<h2>@ViewData["Title"]</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="ListByTech">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <select asp-for="TechnicianID" class="form-control" asp-items="ViewBag.TechnicianID">
                    <option value="" selected>Select Technician</option>
                </select>
            </div>
            <div class="form-group">
                <input type="submit" value="Find Incidents" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

